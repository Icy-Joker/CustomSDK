<?xml version="1.0" encoding="UTF-8"?>
<Root>
  <PreloadModules>
    <!-- Module name="" directory="../bin" enabled="true" / -->
  </PreloadModules>
  <Modules Language="CPlusPlus">
    <!-- Module name="" directory="../test/bin" enabled="true" / -->
  </Modules>
  <!-- 在此处添加自定义配置项 / -->
  <MagicToolBox enable_auto_load="false" auto_load_directory="../plugins/MagicTools">
    <MagicBox name="常规">
      <!-- MagicTool identifier="" location="../plugins/MagicTools" name="" icon="" / -->
      <MagicTool identifier="Plugin_Base64Converter" location="../plugins/MagicTools" name="Base64转换工具" icon="Base64Converter.png" />
      <MagicTool identifier="Plugin_BinaryViewer" location="../plugins/MagicTools" name="二进制查看器" icon="BinaryViewer.png" />
      <MagicTool identifier="Plugin_ChartDemo" location="../plugins/MagicTools" name="图表示例" icon="ChartDemo.png" />
      <MagicTool identifier="Plugin_CryptologyTool" location="../plugins/MagicTools" name="加密与解密示例" icon="CryptologyTool.png" />
    </MagicBox>
  </MagicToolBox>
  <Loggers enabled="true">
    <!-- Logger type="ConsoleLogger|FileLogger|DatabaseLogger" level="trace|debug|info|warn|error|critical|off" format="[%^---%L---%$] [%H:%M:%S %z] [thread %t] %v" / -->
    <Logger type="ConsoleLogger" level="trace" format="[%^---%L---%$] [%H:%M:%S %z] [thread %t] %v" enabled="true" />
    <Logger type="FileLogger" level="trace" format="[%^---%L---%$] [%H:%M:%S %z] [thread %t] %v" enabled="true" />
    <Logger type="DatabaseLogger" level="off" format="[%^---%L---%$] [%H:%M:%S %z] [thread %t] %v" enabled="false" />
  </Loggers>
  <ExtendConfigs enabled="true">
    <!-- ExtendConfig key="" value="" / -->
  </ExtendConfigs>
</Root>
